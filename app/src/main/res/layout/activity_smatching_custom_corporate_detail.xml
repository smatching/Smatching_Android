<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".SmatchingCustomCorporateDetailActivity">
    <RelativeLayout
        android:id="@+id/act_smatching_custom_corporate_detail_rl_toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorBackground">
    <android.support.v7.widget.Toolbar
        android:id="@+id/act_smatching_custom_corporate_detail_toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:theme="@style/ThemeOverlay.AppCompat.ActionBar"
        app:popupTheme="@style/ThemeOverlay.AppCompat.Light">



    </android.support.v7.widget.Toolbar>

        <TextView
            android:id="@+id/act_smatching_custom_corporate_detail_toolvar_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text=""
            android:textColor="@color/colorText"
            android:fontFamily="@font/nanumsquareb"
            android:textSize="18sp"/>
    </RelativeLayout>

    <ScrollView
        android:layout_below="@+id/act_smatching_custom_corporate_detail_rl_toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_marginTop="17dp"
            android:paddingBottom="30dp">

            <ImageView
                android:id="@+id/fragment_smatching_custom_corporatedetail_img_corporatedetail"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_marginBottom="23dp"
                android:src="@drawable/img_corporatedetail" />

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="50dp"
                android:src="@drawable/img_precautionbox" />
        </RelativeLayout>
    </ScrollView>
</RelativeLayout>