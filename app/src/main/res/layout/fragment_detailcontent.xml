<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="90dp"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:background="@color/colorBlue"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
              <LinearLayout
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:orientation="horizontal"
                  android:layout_marginTop="16dp">
                  <TextView
                      android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:layout_marginLeft="24dp"
                      android:layout_marginBottom="11dp"
                      android:text="공고일 | "
                      android:textSize="10sp"
                      android:textColor="@color/colorBackground"
                      android:fontFamily="@font/nanumsquarer"/>
                  <TextView
                      android:id="@+id/fragment_detailcontent_tv_update_date"
                      android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:layout_marginBottom="11dp"
                      android:text=""
                      android:textSize="10sp"
                      android:textColor="@color/colorBackground"
                      android:fontFamily="@font/nanumsquarer"/>
              </LinearLayout>
                <TextView
                    android:id="@+id/fragment_detailcontent_tv_title"
                    android:layout_marginHorizontal="26dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="18sp"
                    android:layout_marginLeft="25dp"
                    android:layout_marginBottom="14dp"
                    android:lineSpacingExtra="5dp"
                    android:text=""
                    android:textColor="@color/colorBackground"
                    android:fontFamily="@font/nanumsquareb"/>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="16dp">
                    <TextView
                        android:id="@+id/fragment_detailcontent_tv_institution"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="25dp"
                        android:layout_centerVertical="true"
                        android:textSize="12sp"
                        android:text=""
                        android:textColor="@color/colorBackground"
                        android:fontFamily="@font/nanumsquarer"/>

                </RelativeLayout>
            </LinearLayout>
        </RelativeLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="25dp"
            android:layout_marginBottom="8dp"
            android:text="지원 기간"
            android:textColor="@color/colorText"
            android:textSize="16sp"
            android:fontFamily="@font/nanumsquareb"/>
        <TextView
            android:id="@+id/fragment_detailcontent_sv_supportperiod"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="25dp"
            android:layout_marginBottom="20dp"
            android:text=""
            android:textSize="14sp"
            android:lineSpacingExtra="5dp"
            android:textColor="@color/colorText"
            android:fontFamily="@font/nanumsquarer"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginBottom="8dp"
            android:textColor="@color/colorText"
            android:text="사업 요약"
            android:textSize="16sp"
            android:fontFamily="@font/nanumsquareb"/>
        <TextView
            android:id="@+id/fragment_detailcontent_sv_summary"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="25dp"
            android:layout_marginBottom="20dp"
            android:text=""
            android:textSize="14sp"
            android:lineSpacingExtra="5dp"
            android:textColor="@color/colorText"
            android:fontFamily="@font/nanumsquarer"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginBottom="8dp"
            android:textColor="@color/colorText"
            android:text="지원 대상"
            android:textSize="16sp"
            android:fontFamily="@font/nanumsquareb"/>
        <TextView
            android:id="@+id/fragment_detailcontent_sv_target"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="25dp"
            android:layout_marginBottom="20dp"
            android:text=""
            android:textSize="14sp"
            android:lineSpacingExtra="5dp"
            android:textColor="@color/colorText"
            android:fontFamily="@font/nanumsquarer"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginBottom="8dp"
            android:textColor="@color/colorText"
            android:text="지원 내용"
            android:textSize="16sp"
            android:fontFamily="@font/nanumsquareb"/>
        <TextView
            android:id="@+id/fragment_detailcontent_sv_content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="25dp"
            android:layout_marginBottom="20dp"
            android:text=""
            android:textSize="14sp"
            android:lineSpacingExtra="5dp"
            android:textColor="@color/colorText"
            android:fontFamily="@font/nanumsquarer"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginBottom="8dp"
            android:text="주관기관 담당부서"
            android:textColor="@color/colorText"
            android:textSize="16sp"
            android:fontFamily="@font/nanumsquareb"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/fragment_detailcontent_sv_institution"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="25dp"
                android:layout_marginBottom="4dp"
                android:text=""
                android:lineSpacingExtra="5dp"
                android:textSize="14sp"
                android:textColor="@color/colorText"
                android:fontFamily="@font/nanumsquarer"/>
            <TextView
                android:id="@+id/fragment_detailcontent_sv_department"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="4dp"
                android:layout_marginRight="25dp"
                android:layout_marginBottom="4dp"
                android:text=""
                android:lineSpacingExtra="5dp"
                android:textSize="14sp"
                android:textColor="@color/colorText"
                android:fontFamily="@font/nanumsquarer"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginBottom="30dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="25dp"
                android:layout_marginBottom="4dp"
                android:text="전화번호 :"
                android:textSize="14sp"
                android:textColor="@color/colorText"
                android:fontFamily="@font/nanumsquarer"/>
            <TextView
                android:id="@+id/fragment_detailcontent_sv_phone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="6dp"
                android:layout_marginBottom="4dp"
                android:text= ""
                android:textSize="14sp"
                android:textColor="@color/colorText"
                android:fontFamily="@font/nanumsquarer"/>
        </LinearLayout>
        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingBottom="100dp">
        <ImageView
            android:src="@drawable/img_precautionbox"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"/>
        </RelativeLayout>

    </LinearLayout>
    </ScrollView>


    <RelativeLayout
    android:layout_width="198dp"
    android:layout_height="65dp"
    android:layout_centerHorizontal="true"
    android:layout_alignParentBottom="true"
    android:layout_marginBottom="16dp">
        <ImageView
            android:src="@drawable/floatingbar"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleType="fitXY"/>
            <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp">
            <ImageView
                android:id="@+id/fragment_detailcontent_iv_call"
                android:src="@drawable/icn_detailpage_calll"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="32dp"
                android:layout_marginTop="1dp"
                android:layout_alignParentLeft="true"/>
            <ImageView
                android:id="@+id/fragment_detailcontent_iv_link"
                android:src="@drawable/icn_detailpage_link"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="1dp"
                android:layout_centerHorizontal="true"/>
            <ImageView
                android:id="@+id/fragment_detailcontent_iv_scrap"
                android:src="@drawable/icn_detailpage_scrap_click"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="32dp"/>
        </RelativeLayout>
    </RelativeLayout>
</RelativeLayout>

